{
  "name": "colliejs",
  "private": true,
  "version": "0.0.0",
  "description": "type-safe css-in-js library with zero runtime",
  "author": "colliedog1001@gmail.com",
  "license": "MIT",
  "keywords": [
    "css-in-js",
    "react",
    "zero runtime",
    "typescript",
    "css",
    "styled-components"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest --changedSince=main",
    "test:ci": "jest --ci ",
    "preversion": "pnpm -r build",
    "prepare": "husky install",
    "release": "./scripts/cli.mjs release"
  },
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "dependencies": {
    "@babel/helper-builder-react-jsx": "^7.22.5",
    "@babel/parser": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@babel/types": "^7.22.5",
    "@nindaff/ascii-tree": "^0.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@rollup/pluginutils": "^5.0.2",
    "@types/resolve": "^1.20.2",
    "@unstyled-ui/css": "^1.6.0",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "csstype": "^3.1.2",
    "esprima": "^4.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "npmlog": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolve": "^1.22.2",
    "rollup": "^3.25.1",
    "static-eval": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/generator": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@pnpm/filter-workspace-packages": "^7.0.10",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-strip": "^3.0.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.20.1",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.18",
    "@types/npmlog": "^4.1.4",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-localstorage-mock": "^2.4.26",
    "lerna": "^6.6.2",
    "lint-staged": "^13.2.2",
    "organize-imports-cli": "^0.10.0",
    "polished": "^4.2.2",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "semver": "^7.5.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-inspect": "^0.7.42",
    "zx": "^7.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bash -c 'tsc  --project . --noEmit'",
      "eslint --cache --fix"
    ],
    "*.{js,jsx}": [
      "organize-imports-cli"
    ],
    "*.{css,md}": "prettier --write"
  }
}